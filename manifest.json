{
	"manifest_version": 2,

	"name": "tabby",
	"description": "This extension makes it easy to send Chrome sessions to other users.",
	"version": "1.0",
	"icons": { 
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
		},
	"browser_action": {
		"default_icon": "icon19.png",
		"default_popup": "popup.html"},
	"permissions": [
		"tabs"]
}